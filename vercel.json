{
  "version": 2,
  "builds": [
    {
      "src": "ml_api/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "ml_api/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "ml_api.settings",
    "DEBUG": "False"
  }
}
